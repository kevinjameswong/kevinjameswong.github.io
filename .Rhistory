math234_grade = function(groupwork, hw, quizzes, exam1, exam2, exam3, final, replace) {
overall = groupwork * 0.05 + hw * 0.10 + quizzes * 0.10 + exam1 * 0.15 + exam2 * 0.15 + exam3 * 0.15 + final * 0.3
grade = function(input) {
if (input < 60) {
letter = "F"
}
else if (input < 63 + 1/3) {
letter = "D-"
}
else if (input < 66 + 2/3) {
letter = "D"
}
else if (input < 70) {
letter = "D+"
}
else if (input < 73 + 1/3) {
letter = "C-"
}
else if (input < 76 + 2/3) {
letter = "C"
}
else if (input < 80) {
letter = "C+"
}
else if (input < 83 + 1/3) {
letter = "B-"
}
else if (input < 86 + 2/3) {
letter = "B"
}
else if (input < 90) {
letter = "B+"
}
else if (input < 93 + 1/3) {
letter = "A-"
}
else if (input < 96 + 2/3) {
letter = "A"
}
else {
letter = "A+"
}
return(letter)
}
prefinal = (groupwork * 0.05 + hw * 0.10 + quizzes * 0.10 + exam1 * 0.15 + exam2 * 0.15 + exam3 * 0.15) / (0.05 + 0.10 + 0.10 + 0.15 + 0.15 + 0.15)
prex3 = list(prefinal, grade(prefinal))
overallex = list(overall, grade(overall))
names(prex3) = c("Pre Final Grade", "Letter")
names(overallex) = c("Overall Grade", "Letter")
result = list(prex3, overallex)
return(result)
}
